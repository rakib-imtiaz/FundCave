                        <TableColumn fx:id="a_id_column" prefWidth="155.0" text="anonymous_ID" />
                        <TableColumn fx:id="timeColumn" prefWidth="178.0" text="time" />
                        <TableColumn fx:id="postColumn" prefWidth="336.0" text="post" />
                        <TableColumn fx:id="communicate" minWidth="0.0" prefWidth="203.0" text="Communicate" />
                    </columns>
                </TableView>
            </content>
        </ScrollPane>
      <Button fx:id="goBackBtn" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
