            <Font name="System Bold" size="10.0" />
         </font>
      </Button>
      <Button fx:id="forgetPasswordBtn" alignment="CENTER" layoutX="147.0" layoutY="408.0" mnemonicParsing="false" onAction="#forgetPassword" prefHeight="33.0" prefWidth="130.0" style="-fx-background-color: white; -fx-background-radius: 10;" text="Forget Passsword?" textFill="#da7618">
         <font>
            <Font name="System Bold" size="10.0" />
         </font>
      </Button>
      <Label layoutX="212.0" layoutY="84.0" prefHeight="37.0" prefWidth="85.0" text="Portal" textAlignment="CENTER" textFill="#fc8014">
         <font>
            <Font name="FreeSerif" size="32.0" />
         </font>
      </Label>
      <ImageView fitHeight="51.0" fitWidth="154.0" layoutX="146.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../logo.jpg" />
         </image>
      </ImageView>
